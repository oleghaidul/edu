<h1>Courses#show</h1>
<p>Find me in app/views/courses/show.html.erb</p>
<%= content_for :sidebar do %>
  <div class="span3">
    <ul class="nav nav-list">
      <li class="nav-header">
        Lessons
      </li>
      <% @course.lessons.each do |lesson| %>
        <li class="tab_link"><%= link_to lesson.name, [@course, lesson] %></li>
      <% end %>
    </ul>
  </div>
<% end %>