<div class="block">        
  <div class="content">
    <div class="inner">
      
        <p>
          <%= raw yt_client.my_video(@video.yt_video_id).embed_html %>
        </p>
        <h3><%= @video.description %></h3>
        <p><b>url:</b> <a href="http://www.youtube.com/watch?v=<%= @video.yt_video_id %>&feature=youtube_it" target="_blank">http://www.youtube.com/watch?v=<%= @video.yt_video_id %></a></p>

      <div class="wat-cf">
        <%= link_to "Edit", edit_video_path(@video), :class => "button" %> 
        <%= link_to "Delete", video_path(@video), :method => "delete", :class => "button", :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
      </div>      
    </div>
  </div>

</div>


<%= render "sidebar" %>